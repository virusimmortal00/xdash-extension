{
  "compilerOptions": {
    "outDir": "./dist/xdash-extension-release",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "allowJs": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "lib": ["es2017", "dom"]
  },
  "include": ["./**/*.ts"]
}